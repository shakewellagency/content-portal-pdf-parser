{
    "name": "shakewellagency/content-portal-pdf-parser",
    "description": "This is my package content-portal-pdf-parser",
    "keywords": [
        "shakewellagency",
        "content-portal-pdf-parser"
    ],
    "homepage": "https://github.com/shakewellagency/content-portal-pdf-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "kylyn.luterte",
            "email": "kylyn.l@shakewell.agency",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.9",
        "spatie/laravel-activitylog": "^4.9"
    },
    "require-dev": {
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Shakewellagency\\ContentPortalPdfParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shakewellagency\\ContentPortalPdfParser\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shakewellagency\\ContentPortalPdfParser\\ServiceProviders\\ParserServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
